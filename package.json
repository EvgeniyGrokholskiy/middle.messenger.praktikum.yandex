    "sinon": "^16.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "dependencies": {
    "express-history-api-fallback": "^2.2.1",
    "handlebars": "^4.7.8",
    "nanoid": "^4.0.2",
    "normalize.css": "^8.0.1"
